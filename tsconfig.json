{
    "version": "1.8.0",
    "compilerOptions": {
        "typeRoots" : ["./typings"],
        "outDir": "demoBack",
        "rootDir": "src",
        "lib": [ "es5" ],
        "target": "ES5",
        "module": "commonjs",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": false,
        "noImplicitAny": true,
        "declaration": false,
        "removeComments":true
    },
    "exclude": [ "build", "node_modules" ]
}
